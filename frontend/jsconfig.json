{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@components/*": ["src/components/*"],
            "@assets/*": ["src/assets/*"],
            "@pages/*": ["src/pages/*"],
            "@services/*": ["src/services/*"],
            "@stores/*": ["src/stores/*"],
            "@hooks/*": ["src/hooks/*"],
            "@styles/*": ["src/styles/*"],
            "@icons/*": ["src/icons/*"],
            "@layouts/*": ["src/layouts/*"],
            "@contexts/*": ["src/contexts/*"],
            "@containers/*": ["src/containers/*"],
            "@utils/*": ["src/utils/*"]
        }
    },
    "include": ["src/**/*"]
}
